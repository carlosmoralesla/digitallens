<tr class="nested-fields">
  <td><%= f.number_field :quantity,:value=>1, style: 'width:5em', class: 'form-control validate[optional]',:readonly=>true %></td>
  <td><%= f.collection_select :lent_serial_id, @lent_serials, :id, :str_name_sale,{}, style: 'width:20em',class: 'selection' %></td>
  <td><%= f.text_field :price_sale, style: 'width:10em', class: 'form-control validate[custom[number]]' %></td>
  <td><%= link_to_remove_association "<img src='/assets/icons/delete.png' width='14' height='14'>".html_safe, f %></td>
</tr>
      